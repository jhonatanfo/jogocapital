        color: #ffffff;
        background: #1b6bb3;
        border: 0px solid #013ADF;
    }

    .linha {
        margin-top: 40px;
    }

        .linha img {
            object-fit: cover;
            margin-top: 20px;
            width: 100%;
            height: 100%;
        }

    .sorteio {
        font-family: "Tw Cen MT Std Extra Bold";
        height: 900px;
        width: 1100px;
        font-size: 40px;
        background: #dbdddb;
        color: #000000;
        margin-top: 50px;
        border-radius: 35px;
    }

        .sorteio p {
            font-size: 80px;
            color: #1b6bb3;
            margin-top: -30px;
        }

    .triang {
        justify-content: center;
        display: flex;
        flex-direction: row;
        font-size: 50px;
    }

    .fa-play {
        margin-top: 20px;
        font-size: 50px;
        margin-right: 150px;
        margin-left: 150px;
        color: #1b6bb3;
    }

    .dias {
        flex-direction: row;
        display: flex;
        margin-top: 20px;
    }

    .Dsort {
        font-family: "Tw Cen MT Std Extra Bold";
        margin-left: 20px;
        margin-right: 20px;
        border: none;
        font-size: 20px;
        width: 180px;
        height: 30px;
        border-radius: 35px;
        background: #ffffff;
    }

    .hr {
        font-family: "Tw Cen MT Std Extra Bold";
        margin-left: 20px;
        margin-right: 20px;
