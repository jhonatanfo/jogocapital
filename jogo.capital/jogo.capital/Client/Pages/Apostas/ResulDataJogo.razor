        display: flex;
        height: 8px;
        width: 450px;
        background: #1b6bb3;
        border: none;
        margin-right: 10px;
        margin-left: 10px;
        margin-top: 20px;
    }

    .text {
        font-family: "Tw Cen MT Std Extra Bold";
        font-size: 20px;
        border: none;
        background: #D8D8D8;
        color: #1b6bb3;
        text-align: center;
        margin-top: 10px;
    }

    .lado-lado {
        display: flex;
        margin-top: -20px;
        align-items: center;
        align-content: center;
        justify-content: center;
    }

    .prim {
        margin-top: 30px;
        width: 1500px;
        flex-direction: column;
        align-items: center;
        align-content: center;
        justify-content: center;
    }

    .seg {
        margin-top: 30px;
        width: 1000px;
        flex-direction: column;
        align-items: center;
        align-content: center;
        justify-content: center;
    }

    .dia {
        display: flex;
        height: 35px;
        width: 520px;
        margin-right: 20px;
        margin-top: 5px;
        border-radius: 35px;
        border: none;
        background: #ffffff;
        align-items: center;
        align-content: center;
        justify-content: center;
    }

        .dia h1 {
            margin-left: 10px;
            font-size: 20px;
            font-family: "Tw Cen MT Std Extra Bold";
        }

        .dia h2 {
            font-size: 20px;
            font-family: "Tw Cen MT Std Extra Bold";
            color: #FABA0E;
        }

        .dia h3 {
            font-size: 20px;
            font-family: "Tw Cen MT Std Extra Bold";
            margin-left: auto;
            margin-right: 10px;
        }


    footer {
        display: block;
        color: #ffffff;
        height: 100%;
        font-family: "Tw Cen MT Std";
    }

    .rodape {
        height: 300px;
        margin-top: 50px;
        flex-direction: row;
        background: #292d2f;
        display: grid;
        grid-template-columns: repeat(3,1fr);
    }

    .copy p {
        margin-top: 260px;
        font-size: 13px;
        margin-left: 10px;
    }

    .redes {
        margin-top: 70px;
        text-align: center;
        align-items: center;
        justify-content: center;
        font-size: 13px;
    }

        .redes i {
            margin-top: 15px;
            text-align: center;
            align-items: center;
            font-size: 30px;
            margin-right: 15px;
            margin-left: 15px;
            margin-bottom: 15px;
        }

        .redes h2 {
            margin-top: 15px;
        }

        .redes .um {
            width: 140px;
            height: 70px;
        }

        .redes .dois {
            width: 140px;
            height: 58px;
            margin-bottom: 6px;
        }

    .direita {
        margin-top: 50px;
        margin-left: 180px;
        font-size: 12px;
        letter-spacing: 0.5px;
